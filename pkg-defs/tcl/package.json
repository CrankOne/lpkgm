{
  "version-regex": [
    "^(?P<major>[0-9]+)\\.(?P<minor>[0-9]+)\\.(?P<patch>[0-9]+)$"
  ],
  "install-stages": [
    {
      "type": "shell-cmd",
      "cmd": ["{pkgDir}/build.sh", "{major}.{minor}.{patch}", "{lpkgm-prefix}/common"],
      "files": {
        "installed-files-list": ["tcl.src/unix/installed-files.txt"]
      }
    }
  ]
}
